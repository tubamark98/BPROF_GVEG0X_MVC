<Window x:Class="ApiConsumer.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApiConsumer"
        mc:Ignorable="d"
        Title="PasswordWindow" Height="250" Width="350">
    <StackPanel>
        <Label Content="Username" Background="LightBlue" Margin="20" Width="300"/>
        <TextBox x:Name="tb_username" Width="300"/>
        <Label Content="Password" Background="LightBlue" Margin="20" Width="300" />
        <PasswordBox x:Name="tb_pass" Width="300"/>
        <Button Content="Login" Click="Login_Event" Width="100"/>
    </StackPanel>
</Window>
